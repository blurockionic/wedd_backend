  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": " nodemon server.js",
    "prisma-generate": "prisma generate --schema=prisma/schema.postgre.prisma",
    "prisma:generate-mongo": "prisma generate --schema=prisma/schema.mongo.prisma",
    "prisma:generate-all": "npm run prisma-generate && npm run prisma:generate-mongo",
    "prisma:dbpush-postgres": "npx prisma db push --schema=./prisma/schema.postgre.prisma",
    "prisma:dbpush-mongo": "npx prisma db push --schema=prisma/schema.mongo.prisma",
    "prisma:dbpush-all": "npm run prisma:dbpush-postgres && npm run prisma:dbpush-mongo",
    "prisma:migrate": "prisma migrate dev --schema=prisma/schema.postgre.prisma",
    "prisma:migrate-mongo": "prisma migrate dev --schema=prisma/schema.mongo.prisma",
    "prisma:migrate-all": "npm run prisma:migrate && npm run prisma:migrate-mongo"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cloudinary": "^1.41.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.9.16",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8"
  },
